# Pict Model

char origin file: 'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h'
int origin rank: 1, 2, 3, 4, 5, 6, 7, 8
char destination file: 'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h'
int destination rank: 1, 2, 3, 4, 5, 6, 7, 8
ChessColor turn color: BLACK, WHITE
long elapsed time: Long.MIN_VALUE, -1L, 0L, 1L, Long.MAX_VALUE
Piece.PromotionChoice promotionChoice: null, PROMOTE_TO_ROOK, PROMOTE_TO_KNIGHT, PROMOTE_TO_BISHOP, PROMOTE_TO_QUEEN

#
# for these tests we are not trying to move to the same space
#
IF [char origin file] = [char destination file]
THEN [int origin rank] <> [int destination rank];

